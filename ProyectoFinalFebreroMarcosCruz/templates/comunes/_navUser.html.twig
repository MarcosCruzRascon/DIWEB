<header>
	<!-- Header Start -->
	<div class="header-area">
		<div class="main-header header-sticky">
			<div class="container-fluid">
				<div
					class="menu-wrapper">
					<!-- Logo -->
					<div class="logo">
						<a href="{{path('index')}}"><img src="assets/img/logo/logo.png" alt=""></a>
					</div>
					<!-- Main-menu -->
					<div class="main-menu d-none d-lg-block">
						<nav>
							<ul id="navigation">
								<li>
									<a href="{{path('index')}}">Pagina Principal</a>
								</li>
								<li>
									<a href="{{path('antonio')}}">DIWEB</a>
								</li>
							</ul>
						</nav>
					</div>
					<!-- Header Right -->
					<div class="header-right">
						<ul>
							{% if app.user == null %}
								<li>
									<a href="{{path('app_register')}}">
										<span class="flaticon-user">Registrar</span>
									</a>
								</li>
								<li>
									<a href="{{path('app_login')}}">
										<span class="flaticon-user">Login</span>
									</a>
								</li>
								<li>
									<a data-bs-toggle="modal" data-bs-target="#exampleModal">
										<span class="flaticon-shopping-cart"></span>
									</a>
								</li>
							{% else %}
								<span class="user-icon">
									<img class="rounded-circle" height="42" width="42" src="{{asset('uploads/imagenes/usuarios/')}}/{{app.user.id}}/{{app.user.imagen}}" alt="">
								</span>
								<span class="user-name">{{ app.user.nombre }}</span>
								<span>
									<li>
										<a href="{{path('app_logout')}}">
											<span class="flaticon-user">Logout</span>
										</a>
									</li>
								</span>
								<span>
									<li>
										<a data-bs-toggle="modal" data-bs-target="#exampleModal">
											<span class="flaticon-shopping-cart"></span>
										</a>
									</li>
								</span>
							{% endif %}
							<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
								<div class="modal-dialog">
									<div class="modal-content">
										<div class="modal-header">
											<h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
											<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close">X</button>
										</div>
										<div class="modal-body carrito">
											<table class="table">
												<thead>
													<tr>
														<th scope="col">Imagen</th>
														<th scope="col">Producto</th>
														<th scope="col">Precio</th>
														<th scope="col">Cantidad</th>
													</tr>
												</thead>
												<tbody id="productos">
												</tbody>
											</table>
											<span>Total</span><span id="total"></span>
										</div>
										<div class="modal-footer">
											<button type="button" id="limpiar" class="btn btn-primary">Vaciar carrito</button>
											<button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
											<button type="button" id="pagar" class="btn btn-primary">Hacer pedido</button>
										</div>
									</div>
								</div>
							</div>

						</ul>
					</div>
				</div>
				<!-- Mobile Menu -->
				<div class="col-12">
					<div class="mobile_menu d-block d-lg-none"></div>
				</div>
			</div>
		</div>
	</div>
