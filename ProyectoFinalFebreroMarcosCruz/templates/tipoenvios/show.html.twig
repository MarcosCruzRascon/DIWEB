{% extends 'base.html.twig' %}

{% block title %}Tipo de envios{% endblock %}

{% block body %}
    <h1>Tipo de envios</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tipoenvio.id }}</td>
            </tr>
            <tr>
                <th>EmpresaTransporte</th>
                <td>{{ tipoenvio.empresaTransporte }}</td>
            </tr>
            <tr>
                <th>Precio</th>
                <td>{{ tipoenvio.precio }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn btn-outline-info float-left mr-5" href="{{ path('tipoenvios_index') }}">Volver a la lista</a>

    <a class="btn btn-outline-warning float-right" href="{{ path('tipoenvios_edit', {'id': tipoenvio.id}) }}">editar</a>

    {{ include('tipoenvios/_delete_form.html.twig') }}
{% endblock %}
